{
    "name": "oviata/modules",
    "description": "Modules for laravel framework",
    "keywords": [
        "oviata",
        "modules",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.1",
        "laravel/framework": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Oviata\\Modules\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oviata\\Modules\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
